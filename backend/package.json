{
  "name": "whadapp-backend",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon run.ts",
    "test": "mocha --exit -r ts-node/register test/**/*.spec.ts",
    "test-only": "mocha --exit -r ts-node/register",
    "coverage": "nyc mocha --exit -r ts-node/register -r source-map-support/register test/**/*.spec.ts"
  },
  "dependencies": {
    "@types/express": "4.17.6",
    "@types/mongodb": "3.3.16",
    "@wishcore/reason-cli": "1.0.0-beta1",
    "@wishcore/wish-rpc": "0.6.10",
    "@wishcore/wish-sdk": "0.4.0-beta-13",
    "automerge": "0.14.0",
    "bson-buffer": "1.0.2",
    "connect-multiparty": "2.2.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "joi": "17.3.0",
    "mongodb": "3.6.2",
    "ws": "7.4.2"
  },
  "devDependencies": {
    "@types/expect": "1.20.3",
    "@types/joi": "14.3.4",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.16",
    "@types/ws": "7.4.0",
    "eslint": "7.13.0",
    "expect": "23.6.0",
    "mocha": "8.2.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "tslint": "~5.11.0",
    "typescript": "4.1.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "build",
      "dist"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ],
    "report-dir": "build/coverage",
    "all": true
  }
}
